{
  "manifest_version": 2,
  "name": "deadnamekiller",
  "version": "0.001",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "permissions" : ["tabs"],

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/Ionic_Order_Column.png"
    }
  }
}
